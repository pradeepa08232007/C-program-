#include <stdio.h>

int main() {
    int n, i, arr[20];

    printf("Enter number of elements: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter new element: ");
    scanf("%d", &arr[n]);
    n++;

    for(i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}
