#include <stdio.h>
#include <string.h>

int main() {
    char user[20] = "admin";
    char pass[20] = "1234";
    char u[20], p[20];

    printf("Enter username: ");
    scanf("%s", u);
    printf("Enter password: ");
    scanf("%s", p);

    if(strcmp(user, u) == 0 && strcmp(pass, p) == 0)
        printf("Login Successful");
    else
        printf("Invalid Login");

    return 0;
}
